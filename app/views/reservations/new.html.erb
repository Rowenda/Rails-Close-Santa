<h1>reservations new</h1>

<%= simple_form_for([@offer, @reservation]) do |f| %>


    <%= f.input :event_adress %>
    <%= f.input :reservation_start, as: :time, html5: true %>
    <%= f.input :reservation_end, as: :time, html5: true %>
    <%= f.submit %>

<% end %>

<%= link_to 'back to offer', offer_path(@offer) %>
